{
  "name": "bienesraices_mvc",
  "version": "1.0.0",
  "description": "Proyecto de Bienes Raices con express y React",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./index.js",
    "server": "nodemon index.js",
    "css": "postcss public/css/tailwind.css -o public/css/app.css --watch",
    "js": "webpack --watch",
    "dev": "concurrently \"npm run css\" \"npm run js\"",
    "db:import": "node ./seed/seeder.js -i",
    "db:delete": "node ./seed/seeder.js -d"
  },
  "keywords": [
    "node",
    "react",
    "tailwind"
  ],
  "author": "Vera Agustin",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "concurrently": "^7.2.2",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.1",
    "dropzone": "^5.9.3",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.7",
    "pug": "^3.0.2",
    "sequelize": "^6.21.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "nodemon": "^2.0.19",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.1.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
